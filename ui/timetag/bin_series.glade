  <object class="GtkToggleAction" id="y_auto">
    <property name="label">Automatic</property>
    <property name="short_label">Automatic</property>
    <property name="active">True</property>
    <signal name="toggled" handler="y_bounds_changed_cb"/>
  </object>
  <object class="GtkAdjustment" id="y_upper">
    <property name="upper">100000000</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
    <signal name="value_changed" handler="y_bounds_changed_cb"/>
  </object>
  <object class="GtkAdjustment" id="y_lower">
    <property name="upper">100000000</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
    <signal name="value_changed" handler="y_bounds_changed_cb"/>
  </object>
  <object class="GtkAdjustment" id="x_width">
    <property name="lower">0.01</property>
    <property name="upper">100</property>
    <property name="step_increment">1</property>
    <property name="page_increment">10</property>
    <signal name="value_changed" handler="x_width_value_changed_cb"/>
  </object>
